{
  "name": "trello-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "http-proxy-middleware": "^0.19.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.6.1",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.6.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.1",
    "redux-logger": "^3.0.6",
    "redux-starter-kit": "^0.5.1",
    "styled-components": "^4.3.1",
    "tailwindcss": "^1.0.3"
  },
  "scripts": {
    "build:css": "postcss src/tailwind.src.css -o src/index.css",
    "watch:css": "postcss src/tailwind.src.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "now-dev": "react-scripts start",
    "now-build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "postcss-cli": "^6.1.2"
  }
}
